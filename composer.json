{
    "name": "bacart/symfony-aware",
    "description": "Symfony service awareness library",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "symfony",
        "library"
    ],
    "authors": [
        {
            "name": "Alex Bacart",
            "email": "alex@bacart.net"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "doctrine/annotations": "^2.0",
        "doctrine/orm": "^2.16",
        "easycorp/easyadmin-bundle": "^4.7",
        "ergebnis/composer-normalize": "^2.36",
        "friendsofphp/php-cs-fixer": "^3.26",
        "phpstan/phpstan": "^1.10",
        "roave/security-advisories": "dev-latest",
        "symfony/framework-bundle": "6.3.*",
        "symfony/lock": "6.3.*",
        "symfony/mercure": "^0.6",
        "symfony/messenger": "6.3.*",
        "symfony/property-access": "6.3.*",
        "symfony/security-core": "6.3.*",
        "symfony/security-csrf": "6.3.*",
        "symfony/security-http": "6.3.*",
        "symfony/serializer": "6.3.*",
        "symfony/translation": "6.3.*",
        "symfony/validator": "6.3.*",
        "twig/twig": "^3.7"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bacart\\SymfonyAware\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    }
}
