{
    "name": "bacart/symfony-aware",
    "description": "Symfony service awareness library",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "php",
        "symfony",
        "library"
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "doctrine/orm": "^2.11",
        "friendsofphp/php-cs-fixer": "^3.6",
        "phpstan/phpstan": "^1.4",
        "symfony/framework-bundle": "^6.0",
        "symfony/lock": "6.0.*",
        "symfony/mercure": "^0.6",
        "symfony/messenger": "6.0.*",
        "symfony/security-core": "6.0.*",
        "symfony/security-csrf": "6.0.*",
        "symfony/security-http": "6.0.*",
        "symfony/serializer": "6.0.*",
        "symfony/translation": "6.0.*",
        "symfony/validator": "6.0.*",
        "roave/security-advisories": "dev-latest",
        "twig/twig": "^3.3"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "autoload": {
        "psr-4": {
            "Bacart\\SymfonyAware\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alex Bacart",
            "email": "alex@bacart.net"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    }
}
