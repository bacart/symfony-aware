{
    "name": "bacart/symfony-aware",
    "description": "Symfony service awareness library",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "symfony",
        "library"
    ],
    "authors": [
        {
            "name": "Alex Bacart",
            "email": "alex@bacart.net"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "doctrine/annotations": "^2.0",
        "doctrine/orm": "^2.17",
        "easycorp/easyadmin-bundle": "^4.8",
        "ergebnis/composer-normalize": "^2.41",
        "friendsofphp/php-cs-fixer": "^3.48",
        "knpuniversity/oauth2-client-bundle": "^2.18",
        "phpstan/phpstan": "^1.10",
        "roave/security-advisories": "dev-latest",
        "symfony/framework-bundle": "~6.3.0 || ~7.0.0",
        "symfony/lock": "~6.3.0 || ~7.0.0",
        "symfony/mercure": "^0.6",
        "symfony/messenger": "~6.3.0 || ~7.0.0",
        "symfony/property-access": "~6.3.0 || ~7.0.0",
        "symfony/security-core": "~6.3.0 || ~7.0.0",
        "symfony/security-csrf": "~6.3.0 || ~7.0.0",
        "symfony/security-http": "~6.3.0 || ~7.0.0",
        "symfony/serializer": "~6.3.0 || ~7.0.0",
        "symfony/translation": "~6.3.0 || ~7.0.0",
        "symfony/validator": "~6.3.0 || ~7.0.0",
        "twig/twig": "^3.8"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bacart\\SymfonyAware\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "scripts": {
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
